root = true

[*]
charset = utf-8
end_of_line = lf
indent_size = 4
indent_style = space
insert_final_newline = true
trim_trailing_whitespace = true
tab_width = 4

[{*.yaml,*.yml}]
trim_trailing_whitespace = false

[*.md]
trim_trailing_whitespace = false

[*.js]
ij_javascript_spaces_within_imports = true
ij_javascript_spaces_within_object_literal_braces = true
